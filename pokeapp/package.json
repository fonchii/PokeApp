{
  "name": "pokeapp",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "description": "Aplicación de gestión de Pokémon",
  "author": "Christian Pflaumer",
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@reduxjs/toolkit": "^2.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.121",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.2",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.7",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "postinstall": "electron-rebuild",
    "electron": "electron .",
    "dev": "concurrently \"npm:start\" \"npm:electron\"",
    "electron_prod": "electron .",
    "build_react": "react-scripts build",
    "build": "react-scripts build && electron-builder",
    "electron_fail": "wait-on http://localhost:3000 && electron .",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.fonchi.pokeapp",
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "database.js",
      "server.js"
    ],
    "extraFiles": [
      
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron/rebuild": "^3.7.1",
    "@types/electron": "^1.6.12",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "wait-on": "^8.0.1"
  }
}
